.clock{
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
}
.clock text{
    color: #fff;
    font-size: 26rpx;
    position: absolute;
    top: 48%;
    left: 50%;
    transform: translate(-50%,-50%);
}
.clock image{
    width: 480rpx;
    height: 480rpx;
}